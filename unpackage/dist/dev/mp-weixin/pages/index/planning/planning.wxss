}
.study {
	margin-left: 20rpx;
	height: 110rpx;
	margin-top: 10rpx;
}
.study_title {
	white-space: nowrap;
	font-size: 33rpx;
	font-weight: bold;
	letter-spacing: 4rpx;
}
.study_ddl {
	font-size: 30rpx;
	color: grey;
	margin-top: 10rpx;
}
.finish {
	margin-top: 30rpx;
}
.finish_title {
	display: -webkit-box;
