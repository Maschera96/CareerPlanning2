}
.finishitemtitle {
	white-space: nowrap;
	font-size: 33rpx;
	font-weight: bold;
	letter-spacing: 4rpx;
	color: grey;
	text-decoration: line-through;
	/*文字删除线*/
}
.finishitemdetail {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
